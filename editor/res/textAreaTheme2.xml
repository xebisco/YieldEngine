<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<!--
	Dark theme based off of Notepad++'s Obsidian theme.
	See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

    <background color="1D1D26" />
    <caret color="C9C9D1" />
    <selection useFG="false" bg="713a91" roundedEdges="false" />
    <currentLineHighlight color="242433" />
    <marginLine fg="D0D0D9" />
    <markAllHighlight color="713a91" />
    <markOccurrencesHighlight color="5b7179" border="false" />
    <matchedBracket fg="004e4766" bg="4e4766" highlightBoth="true" animate="true" />
    <hyperlinks fg="50AFDE" />
    <secondaryLanguages>
		<language renderIndex="1" bg="333344" />
		<language renderIndex="2" bg="223322" />
		<language renderIndex="3" bg="332222" />
	</secondaryLanguages>

    <!-- Gutter styling. -->
    <gutterBorder color="81969A" />
    <lineNumbers fg="565465" currentFG="565465"/>
    <foldIndicator fg="4A4C54" armedFg="4A4C54" iconBg="1D1D26" iconArmedBg="1D1D26" />
    <iconRowHeader activeLineRange="393747" />

    <!-- Syntax tokens. -->
    <tokenStyles>
        <style token="IDENTIFIER" fg="C9C9D1" />
