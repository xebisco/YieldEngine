        <style token="FUNCTION" fg="93A6F5" />
        <style token="DATA_TYPE" fg="93A6F5" bold="true" />
        <style token="LITERAL_BOOLEAN" fg="E0957B" />
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="4DACF0" />
        <style token="LITERAL_NUMBER_FLOAT" fg="4DACF0" />
